{
  "name": "araneo",
  "version": "0.0.2",
  "description": "Helper for validation and getting value by path",
  "main": "dist/main.js",
  "scripts": {
    "start": "nodemon $NODE_DEBUG_OPTION --exec ts-node index.ts",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "clean": "rimraf dist",
    "compile": "npm run clean && tsc",
    "lint": "standard *.ts"
  },
  "author": "Pokhodyun Alexander",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "chai": "^4.1.2",
    "eslint-plugin-typescript": "^0.8.1",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "typescript-eslint-parser": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/karbunkul/araneo.git"
  },
  "bugs": {
    "url": "https://github.com/karbunkul/araneo/issues",
    "email": "karbunkul@yourtask.ru"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "ignore": [
      "tests/**/*"
    ]
  }
}
